{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "dist",
    "baseUrl": "src",
    "sourceMap": true,
    "noImplicitAny": true,
    "strict": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*.ts","src/**/*.json",],
  "exclude": ["node_modules"]
}
